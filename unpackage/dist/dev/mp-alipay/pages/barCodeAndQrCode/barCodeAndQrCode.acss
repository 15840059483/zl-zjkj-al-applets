.payment-list-item {
  min-height: .8rem;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10px 0.5rem !important;
}
.payment-list-item ._div {
  word-wrap: break-word;
  word-break: break-all;
}
.payment-list-item ._div:first-child {
  min-width: 2.2rem;
}
.payment-list-item .register-yellow {
  font-size: 14px;
  color: #ffc925;
}
.payment-list-item .register-yellow ._div:first-child {
  padding-bottom: .15rem;
}
.margin-top {
  margin-top: .5rem;
}
.el-message-box {
  width: 80%;
}
.el-icon-s-home {
  color: #409EFF;
}
.hometitle {
  width: 100%;
  height: 1.3rem;
  line-height: 1.3rem;
  text-align: center;
  font-size: .5rem;
  color: #ffffff;
  background: #67C23A;
  position: fixed;
  top: 794px;
  z-index: 999;
}
.el-icon-arrow-left {
  position: absolute;
  font-size: .7rem;
  left: .5rem;
  top: 50%;
  -webkit-transform: translate(0, -50%);
          transform: translate(0, -50%);
}
.homeh-btn {
  position: absolute;
  right: 0;
  bottom: 0;
  height: 100%;
  padding: 0 .3rem;
  font-size: .4rem;
}
.buttom_height {
  height: 100rpx;
}
.tab_button {
  width: 100%;
  position: fixed;
  z-index: 1;
  bottom: 0rpx;
  display: flex;
  height: 110rpx;
  text-align: center;
  color: #ffffff;
  background-color: #67C23A;
  box-shadow: 0rpx 4rpx 8rpx 0rpx;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.tab_button .tab_item {
  margin-top: 10rpx;
  width: 50%;
  font-size: 24rpx;
  color: #666666;
}
.tab_button .tab_item image {
  width: 48rpx;
  height: 48rpx;
}
.tab_button .tab_item .active_item {
  color: #d81e06;
}

