  right: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 1000;
}
.del-container {
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 1000;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  border-radius: .3rem;
  width: 6rem;
  font-size: .4rem;
  letter-spacing: .05rem;
}
.del-container .del-container-row {
  border-bottom: 1px solid #cdcdcd;
  padding: .1rem 0;
}
.del-container .del-container-cancel {
  border-right: 1px solid #cdcdcd;
}
.del-container .del-container-col {
  padding: .3rem 0;
}
.del-container .del-container-del {
  color: #ff6633;
