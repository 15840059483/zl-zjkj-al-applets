.outpatient-department-container {
  margin-bottom: .4rem;
}
.outpatient-department-container .outpatient-department {
  color: #4a4a4a;
}
.outpatient-department-container .outpatient-department ._img {
  vertical-align: center;
  width: 16px;
  height: 16px;
  margin-right: .2rem;
}
.outpatient-department-container .introduce {
  color: #b4b4b4;
  font-size: 16px;
  line-height: .5rem;
}
.data-container {
  margin-bottom: 2px;
}
.data-container .data-change-btn ._div {
  float: left;
}
.data-container .icon-caret-bor {
  width: 12px;
  height: 12px;
  border: 1px solid;
  border-radius: 50%;
  text-align: center;
  color: #b4b4b4;
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin: 2px 0 0 16px;
}
.data-container .data-change-r {
  float: right;
  display: flex;
  color: #008cfe;
  font-size: .35rem;
  border: 1px solid #008cfe;
}
.data-container .data-change-r > ._div {
  padding: .05rem .2rem;
}
.data-container .data-change-r .selected-bg {
  background-color: #008cfe;
  color: #ffffff;
}
.data-container .date-week {
  width: 14.2%;
  text-align: center;
  font-size: 16px;
  border-top: 1px solid #e1e1e1;
  border-bottom: 1px solid #e1e1e1;
  padding: .2rem 0;
}
.data-container .date-day {
  width: 100%;
  text-align: center;
  margin: .3rem auto;
  padding: .05rem;
  font-size: 12px;
  line-height: 1rem;
}
.data-container .date-day .date-have {
  font-size: .25rem;
  color: #888787;
}
.data-container .select-date {
  background-color: #409EFF;
  color: #ffffff;
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}
.data-container .select-date .date-have {
  color: #ffffff;
}
.doc-row {
  padding: .3rem;
}
.doc-row .doc-img {
  width: 65px;
  height: 85px;
  margin: 0 auto;
  background-color: #e1e1e1;
}
.doc-row .doc-img ._img {
  width: 100%;
  height: 100%;
}
.doc-row .doc-name {
  font-size: 16px;
  line-height: 16px;
}
.doc-row .doc-position {
  font-size: 16px;
  color: #b4b4b4;
}
.doc-row .doc-money {
  font-size: 16px;
  line-height: 1.5rem;
  text-align: center;
  padding-right: .3rem;
  color: #ec6c25;
}
.doc-row .doc-remainder {
  background-color: #008cfe;
  color: #ffffff;
  font-size: .3rem;
  padding: .05rem .2rem;
  line-height: 1.7rem;
  border-radius: .1rem;
}
.doc-row:not(:last-child) {
  border-bottom: 1px solid #e1e1e1;
}
.confirm-div {
  position: fixed;
  top: 0;
  right: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 1000;
}
.confirm-container {
  width: 100%;
  position: fixed;
  bottom: 0;
  z-index: 1001;
  overflow: hidden;
}
.confirm-container .confirm-doc {
  font-size: .4rem;
  background-color: #008cfe;
  color: #ffffff;
  padding: .3rem 0;
}
.confirm-container .confirm-time {
  padding: .4rem .6rem;
  display: flex;
}
.confirm-container .confirm-time ._div {
  color: #ffffff;
  padding: .15rem .3rem;
  background-color: #008cfe;
  margin-right: .5rem;
}
.confirm-container .confirm-btn-row {
  border-top: 1px solid #e1e1e1;
}
.confirm-container .confirm-btn-row .confirm-btn {
  padding: .5rem 0;
}
.confirm-container .confirm-btn-row .confirm-btn-save {
  border-left: 1px solid #e1e1e1;
  color: #008cfe;
}


































































































































































































































































































































































































































































































